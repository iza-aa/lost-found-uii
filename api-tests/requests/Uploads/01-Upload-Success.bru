meta {
  name: TC-UPLOAD-001 Upload Image Success
  type: http
  seq: 1
}

post {
  url: {{base_url}}/api/{{api_version}}/upload
  body: multipartForm
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:multipart-form {
  file: @file(C:\Windows\Web\Wallpaper\Theme1\img1.jpg)
}

tests {
  test("Status is 200", function() {
    expect(res.status).to.equal(200);
  });
  
  test("Returns URL", function() {
    expect(res.body.url).to.be.a('string');
  });
}

docs {
  Select any valid image file (JPG/PNG) smaller than 2MB.
  The path above is a default Windows wallpaper path, change it if needed.
}

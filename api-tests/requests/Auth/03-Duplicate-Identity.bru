meta {
  name: TC-AUTH-005 Duplicate Identity Number
  type: http
  seq: 3
}

post {
  url: {{base_url}}/api/{{api_version}}/auth/register
  body: json
  auth: none
}

body:json {
  {
    "name": "Different Person",
    "email": "21523120different@students.uii.ac.id",
    "password": "test123",
    "phone": "08123456789",
    "identity_number": "21523120",
    "role": "MAHASISWA",
    "faculty": "Fakultas Teknologi Industri"
  }
}

tests {
  test("Status is 400", function() {
    expect(res.status).to.equal(400);
  });
  
  test("Returns correct error", function() {
    expect(res.body.error).to.include("identity number already registered");
  });
}
